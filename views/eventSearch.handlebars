	<title>hApp</title>

	<link rel="stylesheet" href="/scripts/bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" href="/scripts/bootstrap-material-design/dist/css/bootstrap-material-design.css">
	<link rel="stylesheet" href="/static/css/main.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>

<body class="bg-light">
	{{!-- Begin here (or replace this with any static code already written). --}}


	<nav class="navbar navbar-expand-lg navbar-light bg-light">
		<div class="navbar-header">
			<a class="navbar-brand center" href="/home"><h3>hApp</h3></a>
			<a href="/feelingSelect" class="left">
				<button type="button" class="btn btn-lg btn-header">
					<img src="/static/icons/back-arrow.svg" height="30"/>
				</button></a>
		</div>
	</nav>
	{{#if viewAlt}}
	<div class="container">
        <div class="row">
            <div class="col-xs-12 col-md-12">
                {{#each events}}
                <a class="thumbnail" style="width: 100%; height: 50px; overflow: auto;">
                    <img src={{imageURL}} style="width: 100%; height: 200px; display: block;">
                    <div class="caption" style="padding-bottom:20px">
                        <h5>{{name}}</h5>
                        <h6>{{distance}}</h6>
                        <p>{{description}}</p>
                        <p>
                            <a href="/map" class="pull-left">
                            <i class="fa fa-heart" style="color:#000"></i>
                            </a>
                            &nbsp;&nbsp;&nbsp;
                            <a href="/spider" class="pull-right">
                            <img src="/static/icons/ellipsis.svg" height="20" />
                            </a>
                        </p>
                    </div>
                </a>
                {{/each}}
            </div>
        </div>
    </div>

	{{else}}
	<div class="container" style="height:calc(77% - 0.5pt); padding-top: 2.5%;">
		<div class="card" style="text-align:center; margin-top:0; margin-left:0; margin-right:0; height: 100%;">
			<img class="card-img-top crop img-responsive eventImage" src="" style="height:70%;">
			<div class="card-body text-left">
				<h6 class="card-text eventName" style="margin-bottom: 0;"></h6>
				<span class="card-text eventDistance" style="font-weight:bolder;"></span>
				<p class="card-text eventDescription" style="margin-top:0.5em;"></p>
			</div>
		</div>
	</div>

	<div class="outer" id="outer">
		<a><button class="btn inner rewind"><i class="fa fa-arrow-left"></i></button></a>
		&nbsp;&nbsp;&nbsp;
		<a><button class="btn inner btn-circle btn-xl uninterested"><i class="fa fa-close"></i></button></a>
		&nbsp;&nbsp;&nbsp;
		<a href="/map"><button class="btn inner btn-circle btn-xl"><i class="fa fa-heart" style="color:#000"></i></button></a>
		&nbsp;&nbsp;&nbsp;
		<a href="/spider"><div class="inner"><button type="button" class="btn btn-secondary"><img src="/static/icons/ellipsis.svg" height="20"/></button></div></a>
	</div>
	{{/if}}

	

	<script src="/scripts/jquery/dist/jquery.js"></script>
	<script src="/scripts/bootstrap/dist/js/bootstrap.js"></script>
	<script src="/scripts/popper.js/dist/umd/popper.js"></script>
	<script src="/scripts/bootstrap-material-design/dist/js/bootstrap-material-design.js"></script>
	<script src="/static/js/eventSearch.js"></script>
