<!doctype html>

<html>
<head>
  <title>Task</title>  
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- this is a comment in HTML -->  
  <!-- Bootstrap -->
  <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <script type="text/javascript" src="/js/jquery.timepicker.js"></script>
  <link rel="stylesheet" type="text/css" href="/css/jquery.timepicker.css">
  <link href="/css/bootstrap.min.css" rel="stylesheet">
  <link href="/css/add.css" rel="stylesheet">
  <script src="/js/taskAdd.js"></script>
</head>

<body>
<div class="container">
  <button type="button" class="btn btn-default btn-sm btn-back">
     <a href="/"><span class="glyphicon glyphicon-arrow-left"></span></a>
  </button>
  <h1><font color="white">Task</font></h1>

<form id-"eventInput">
  <div class="form-group row">
    <label for="title" class="col-sm-2 col-form-label"><font color="white">Title</font></label>
    <div class="col-sm-10">
      <input type="title" class="form-control" id="titleForm" placeholder="ex. Homework, Laundry, etc.">
    </div>
  </div>
  <div class="form-group row">
    <label for="start" class="col-sm-2 col-form-label"><font color="white">Start By</font></label>
    <div class="col-sm-10">
      <input type="start" class="form-control timepicker" id="startForm">
    </div>
    <script>
     $(document).ready(function(){
        $('input.timepicker').timepicker({});
      });
    </script>
  </div>
  <div class="form-group row sub">
    <label for="duration" class="col-sm-2 col-form-label"><font color="white">Duration</font></label>
    <div class="col-sm-10">     
    <select id="durationForm" class="form-control">
      <option>0.25 hr</option>
      <option>0.5 hr</option>
      <option>0.75 hr</option>
      <option>1 hr</option>
    </select>
    </div>
  </div>
  <div class="form-group row">
    <label for="start" class="col-sm-2 col-form-label"><font color="white">Deadline</font></label>
    <div class="col-sm-10">
      <input type="end" class="form-control timepicker" id="endAt">
    </div>
    <script>
     $(document).ready(function(){
        $('input.timepicker').timepicker({});
      });
    </script>
  </div>
  <div class="form-group row">
    <label for="priority" class="col-sm-2 col-form-label"><font color="white">Priority</font></label>
    <div class="col-sm-10">     
    <select id="priorityForm" class="form-control">
      <option>High</option>
      <option>Medium</option>
      <option>Low</option>
    </select>
    </div>
  </div>

  
<div class="buttonHolder">
  <!--<button type="button" class="btn btn-primary"><a href="/">Reset</a></button>-->

  <button id="myelement" type="button" class="btn btn-primary btn-add">
      
      <a href="/">Submit</a>
    
  </button>
</div>


