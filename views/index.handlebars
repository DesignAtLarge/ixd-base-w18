	    <title >Home.ie</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <link rel="stylesheet" href="/css/index.css">
    </head>
    <body>
        <div class="container">
            <div class="row navbar">
                <li class="col-xs-4 col-sm-4 col-md-4 col-lg-4 nav_icon">
                    <a href="/stats"> <img style="width: 50px; height: 50px; border-radius: 25px; position:relative; left:-25px;" src="/img/stats.jpg" style="top: 18px"></a>
                </li>
                <li class="col-xs-4 col-sm-4 col-md-4 col-lg-4 nav_icon"><a href="/homepage"><img style="width: 150px; position:relative; left:-25px;" src="/img/logo.png" style="top: 18px"></a></li>
                    {{#if users}}
                    <li class=" col-xs-4 col-sm-4 col-md-4 col-lg-4 nav_icon dropdown pull-right">
                        <a  data-toggle="dropdown" class="dropdown-toggle"><img id="fbProfilePic" user="{{users.0.first}}" src="{{users.0.url}}"><b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li class="divider"></li>
                            <li><a href="/signOut">Sign Off</a></li>
                        </ul>
                    </li>
                    {{else}}
                    <a id="defaultProfile" href="/"><script class="glyphicon glyphicon-user" style="top: 18px" ></script></a>
                    {{/if}}
            </div>

            <div class="row nav_bar">
                    <div class="nav_chore col-sm-6 col-xs-6 col-md-6 col-lg-6">
                        <a id="choresBtn" href="/homepage">
                            <div class="nav">
                                Chores
                            </div>
                        </a>
                    </div>

                    <div class="nav_expense col-sm-6 col-xs-6 col-md-6 col-lg-6">
                        <a id="expensesBtn" href="/expenseHomepage">
                            <div class="nav">
                                Expenses
                            </div>
                        </a>
                    </div>
            </div>
            {{#if viewAlt}}
         <div class="chore_rows row ">
                    {{#each chores}}
                    <div class="chore col-xs-6 col-sm-6 col-md-6 col-lg-6 card">
                        {{#if done}}
                                    <div class="doneGray">
                                {{else}}
                                <div class="chore_content">
                        {{/if}}
                        
                        <div class="row">
                            <div class="des col-xs-4 col-sm-4 col-md-4 col-lg-4">
                            <h3>{{name}}</h3>
                            <p>{{description}}</p>
                        </div>
                            <div class="done col-xs-2 col-sm-2 col-md23 col-lg-2">
                                {{#if done}}
                                <button class="btn btn-danger reviveBtn" index="{{@index}}" type="submit">Revive</button>
                                {{else}}
                                <button class="btn btn-success doneBtn" index="{{@index}}" type="submit">Done</button>
                                {{/if}}
                            </div>

                            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6" style="text-align:center">
                                <span class="glyphicon glyphicon-chevron-down" style="font-size:15px"></span>
                            </div>
                        </div>
                        <canvas width="400" height="400" id="{{@index}}" Karanbir="{{Karanbir}}" Alex="{{Alex}}" All="{{All}}" Youxi="{{Youxi}}" Guest="{{Guest}}" > </canvas>
                        </div>
                    </div>

                    {{/each}}
                </div>

                <div class="add col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    {{#if chores}}
                    <form action="/chore" >
                        <button id="submitBtn" class="btn btn-success btn-block" type="submit"> Create New Chore </button>
                    {{/if}}
                    </form>
                </div>
        </div>
        {{else}}
                         <div class="chore_rows row ">
                    {{#each chores}}
                    <div class="chore col-xs-12 col-sm-12 col-md-12 col-lg-12 card">
                        {{#if done}}
                                    <div class="doneGray">
                                {{else}}
                                <div class="chore_content">
                        {{/if}}
                        
                        <div class="row">
                            <div class="des col-xs-8 col-sm-8 col-md-8 col-lg-8">
                            <h3>{{name}}</h3>
                            <p>{{description}}</p>
                        </div>
                            <div class="done col-xs-4 col-sm-4 col-md43 col-lg-4">
                                {{#if done}}
                                <button class="btn btn-danger doneBtn" state="{{done}}" index="{{@index}}" type="submit">Revive</button>
                                {{else}}
                                <button class="btn btn-success doneBtn" state="{{done}}" index="{{@index}}" type="submit" >Done</button>
                                {{/if}}
                            </div>

                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="text-align:center">
                                <span class="glyphicon glyphicon-chevron-down" style="font-size:15px"></span>
                            </div>
                        </div>
                        <canvas width="400" height="400" id="{{@index}}" Karanbir="{{Karanbir}}" Alex="{{Alex}}" All="{{All}}" Youxi="{{Youxi}}" Guest="{{Guest}}" > </canvas>
                        </div>
                    </div>

                    {{/each}}
                </div>

                <div class="add col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    {{#if chores}}
                    <form action="/chore" >
                        <button id="submitBtn" class="btn btn-success btn-block" type="submit"> Create New Chore </button>
                    {{/if}}
                    </form>
                </div>
        </div>


        {{/if}}
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/3.0.1/handlebars.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.bundle.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.min.js"></script>
        <script type="text/javascript" src="js/bootstrap.js"></script>
        <script type="text/javascript" src="js/index.js"></script>
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-114995147-1"></script>
        <script>
          window.dataLayer = window.dataLayer || [];
          function gtag(){dataLayer.push(arguments);}
          gtag('js', new Date());
          gtag('config', 'UA-114995147-1');
        </script>
    </body>
</html>
